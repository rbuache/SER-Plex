@SuppressWarnings("serial")
public class Horaire extends JPanel{

	JComboBox movieDispo = new JComboBox();
	JComboBox movieSchedule = new JComboBox();
	JTable schedule = new JTable();
	JButton addMovie = new JButton("Ajouter");
	JButton delMovie = new JButton("Supprimer");
	JButton addSchedule = new JButton("Ajouter");
	JButton delSchedule = new JButton("Supprimer");
	
	public Horaire(){
		add(movieDispo);
		 JSeparator north = new JSeparator(JSeparator.HORIZONTAL);
		 add(north);
		add(movieSchedule);
		
	}
}
